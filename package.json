{
	"name": "gulp-starter",
	"version": "1.0.1",
	"description": "An example project for getting started with gulp, including Jasmine unit testing",
	"keywords": [
		"gulp",
		"jasmine",
		"karma",
		"phantomjs"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/sc0tt5/gulp-starter"
	},
	"author": "Scott Robertson (https://github.com/sc0tt5)",
	"license": "ISC",
	"copyright": "Scott Robertson",
	"private": true,
	"main": "gulpfile.babel.js",
	"paths": {
		"src": {
			"base": "./src/",
			"scss": "./src/css/**/*.scss",
			"html": "./src/index.html",
			"img": "./src/img/**/*.{png,jpg,jpeg,gif,svg}",
			"js": "./src/js/**/*.js",
			"vendor": [
				"./node_modules/jquery/dist/jquery.min.js",
				"./node_modules/lodash/lodash.min.js"
			]
		},
		"build": {
			"base": "./build/",
			"css": "./build/css",
			"html": "./build/index.html",
			"img": "./build/img",
			"js": "./build/js",
			"vendor": "./build/js/vendor.js"
		},
		"dist": {
			"base": "./public/",
			"css": "./public/css",
			"html": "./public/index.html",
			"img": "./public/img",
			"js": "./public/js",
			"vendor": "./public/js/vendor.js"
		}
	},
	"devDependencies": {
		"@babel/register": "^7.0.0-beta.51",
		"babel-cli": "^6.26.0",
		"babel-core": "^6.26.3",
		"babel-preset-env": "^1.7.0",
		"babel-register": "^6.26.0",
		"eslint-config-prettier": "^3.0.1",
		"eslint-plugin-jasmine": "^2.10.1",
		"git-rev-sync": "^1.12.0",
		"gulp": "^3.9.0",
		"gulp-autoprefixer": "^5.0.0",
		"gulp-babel": "^7.0.1",
		"gulp-changed": "^3.2.0",
		"gulp-clean": "^0.4.0",
		"gulp-clean-css": "^3.9.4",
		"gulp-concat": "^2.6.1",
		"gulp-connect": "^5.5.0",
		"gulp-eslint": "^5.0.0",
		"gulp-flatten": "^0.4.0",
		"gulp-header": "^2.0.5",
		"gulp-htmlhint": "^2.1.1",
		"gulp-htmlmin": "^4.0.0",
		"gulp-if": "^2.0.2",
		"gulp-imagemin": "^4.1.0",
		"gulp-inject": "^4.3.2",
		"gulp-load-plugins": "^1.5.0",
		"gulp-newer": "^1.4.0",
		"gulp-notify": "^3.2.0",
		"gulp-open": "^3.0.1",
		"gulp-plumber": "^1.2.0",
		"gulp-print": "^5.0.0",
		"gulp-rename": "^1.4.0",
		"gulp-sass": "^4.0.1",
		"gulp-sequence": "^1.0.0",
		"gulp-size": "^3.0.0",
		"gulp-sourcemaps": "^2.6.4",
		"gulp-uglify": "^3.0.0",
		"gulp-watch": "^5.0.1",
		"jasmine-core": "^3.1.0",
		"jshint": "^2.9.5",
		"karma": "^2.0.4",
		"karma-babel-preprocessor": "^7.0.0",
		"karma-coverage": "^1.1.2",
		"karma-jasmine": "^1.1.2",
		"karma-jasmine-html-reporter": "^1.2.0",
		"karma-notify-reporter": "^1.0.1",
		"karma-phantomjs-launcher": "^1.0.4",
		"moment": "^2.22.2",
		"phantomjs": "^2.1.7"
	},
	"babel": {
		"presets": [
			"env"
		]
	},
	"dependencies": {
		"jquery": "^3.3.1",
		"lite-server": "^2.4.0"
	},
	"scripts": {
		"build": "gulp build",
		"dev": "gulp",
		"test": "gulp test"
	}
}
